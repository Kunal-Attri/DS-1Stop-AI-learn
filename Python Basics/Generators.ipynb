   "source": [
    "# kind of fn which can send a value and later continue from where it left off"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "8b2b8b47",
   "metadata": {},
   "outputs": [],
   "source": [
    "def cubes(n):\n",
    "    for i in range(n):\n",
    "        yield i**3"
   ]
  },
